<%- layout('layouts/main.ejs') %>

<div class="container">
  
      <!-- LIST -->
    <div class="col-md-7">
        <table class="table table-bordered table-hover">
          <thead>
            <tr>
              <th>N°</th>
              <th>Servicio</th>
              <th>Solicitar</th>
              <th>Prestador</th>
              <th>Valoración</th>
              
            </tr>
          </thead>
          <tbody>
            <% for(var i=0,j=1; i < services.length; i++) { %>
             <% if (user.tempsearch == services[i].category) { %>
              <tr>
                <td><%= j++%></td>
                <td><%= services[i].title %></td> 
                <td><a href="/requestservice/<%= services[i]._id %>"><button type="button" class="btn btn-light">Solicitar</button></a> %></td>                
                <td></td>
                <td></td>
              </tr>
              <% } %>
            <% } %>
          </tbody>
        </table>
  
      </div>
    </div>
  